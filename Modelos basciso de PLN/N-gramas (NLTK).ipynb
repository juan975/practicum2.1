   "outputs": [],
   "source": [
    "# Texto de ejemplo\n",
    "text = \"Natural Language Processing is fun\"\n",
    "\n",
    "# Tokenizaci√≥n\n",
    "tokens = text.split()\n",
    "\n",
    "# Generar n-gramas (bi-gramas en este caso)\n",
    "bigrams = list(ngrams(tokens, 2))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "fc41ebc1-0cd0-476e-84c6-7217ad56fadd",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Bi-gramas con NLTK: [('Natural', 'Language'), ('Language', 'Processing'), ('Processing', 'is'), ('is', 'fun')]\n"
     ]
    }
   ],
   "source": [
    "print(\"Bi-gramas con NLTK:\", bigrams)"
   ]
  },
